<queue-entries>
	<queue>
		<name>myqueue01</name>
		<rate>1/s</rate>
		<bucket-size>500</bucket-size>
		<max-concurrent-requests>10</max-concurrent-requests>
		<retry-parameters>
			<task-retry-limit>7</task-retry-limit>
			<task-age-limit>2d</task-age-limit>
			<min-backoff-seconds>2</min-backoff-seconds>
			<max-backoff-seconds>3600</max-backoff-seconds>
			<max-doublings>1</max-doublings>
		</retry-parameters>
	</queue>
</queue-entries>
